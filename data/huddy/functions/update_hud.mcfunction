# Update position
execute store result score @p huddy_x run data get entity @p Pos[0]
execute store result score @p huddy_y run data get entity @p Pos[1]
execute store result score @p huddy_z run data get entity @p Pos[2]

# Grab rotation
execute store result score @p huddy_r run data get entity @p Rotation[0]
# Flip 181deg->359deg to -179deg to -1deg
execute if score @p huddy_r > #180 huddy_val run scoreboard players operation @p huddy_r -= #360 huddy_val


